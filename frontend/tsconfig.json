{
  "compilerOptions": {
    "jsx": "react-jsx",  
    "module": "esnext", 
    "target": "es6",  
    "lib": ["dom", "es6"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "isolatedModules": true, 
    "noEmit": true           
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
